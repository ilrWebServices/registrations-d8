{
  "name": "ilr-web-services/registrations",
  "description": "Composer-based Drupal 8 site for register.ilr.cornell.edu",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Jeff Amaral",
      "email": "jeff.amaral@cornell.edu",
      "role": "Developer"
    },
    {
      "name": "Aaron Froelich",
      "email": "aaronf@cornell.edu",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ilrWebServices/union_organizer.git"
    }
  ],
  "require": {
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6.5",
    "drupal/admin_toolbar": "^2.0",
    "drupal/commerce": "^2.15",
    "drupal/commerce_checkout_order_fields": "^1.0",
    "drupal/config_split": "^1.4",
    "drupal/config_update": "^1.7",
    "drupal/console": "^1.0.2",
    "drupal/core-composer-scaffold": "^8.8",
    "drupal/core-recommended": "^8.8",
    "drupal/dynamic_entity_reference": "^2.0",
    "drupal/email_registration": "^1.0@RC",
    "drupal/erf": "^1.0",
    "drupal/fixed_block_content": "^1.0",
    "drupal/key": "^1.14",
    "drupal/queue_ui": "^2.1",
    "drupal/salesforce": "^4.0",
    "drupal/token": "^1.5",
    "drupal/typed_data": "^1.0@alpha",
    "drupal/union_organizer": "dev-master",
    "drush/drush": "^10",
    "firebase/php-jwt": "^5.2",
    "platformsh/config-reader": "^2.2",
    "vlucas/phpdotenv": "^4.1",
    "zaporylie/composer-drupal-optimizations": "^1.0"
  },
  "require-dev": {
    "drupal/core-dev": "^8.8"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "files": ["load.environment.php"]
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
          "web-root": "web/"
      }
    },
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
    },
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {
      "drupal/inline_entity_form": {
        "Field name/label disappears when I create a new entity inline": "https://www.drupal.org/files/issues/inline_entity_form-no_label_required_field_with_no_entries-2842744.patch"
      },
      "drupal/commerce": {
        "Improve order argument for order total Views field": "patches/commerce-order_total_handler.patch",
        "Allow the view in the OrderItemTable formatter to be configured": "https://www.drupal.org/files/issues/2019-08-27/2689919-26_commerce_Order-Items-Table-formatter-select-view-no-post-update.patch"
      },
      "drupal/address": {
        "Configure visible address fields per formatter": "https://www.drupal.org/files/issues/2019-05-23/address-visible_address_fields_per_field_formatter-2852800-23.patch"
      },
      "drupal/commerce_checkout_order_fields": {
        "Required fields not allowed": "https://www.drupal.org/files/issues/2019-06-26/commerce_checkout_order_fields-3063654-2.patch"
      }
    }
  }
}
